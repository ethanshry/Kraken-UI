{
    "name": "kraken-ui",
    "version": "0.1.0",
    "license": "ISC",
    "dependencies": {
        "@ant-design/icons": "^4.5.0",
        "antd": "^4.13.0",
        "graphql": "^15.5.0",
        "parcel": "^2.0.0-beta.1",
        "parcel-bundler": "^1.12.4",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-markdown": "^5.0.3",
        "urql": "^2.0.1"
    },
    "scripts": {
        "start": "npm install && rimraf ./dist && parcel --no-cache index.html",
        "build": "npm install && rimraf ./dist && parcel build index.html --public-url ./ && copyfiles -u 1 ./public/* ./dist",
        "build-web": "npm install && rimraf ./dist/* && webpack --config ./webpack.config.js"
    },
    "devDependencies": {
        "@babel/core": "^7.13.8",
        "@babel/preset-env": "^7.13.8",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.13.0",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.1",
        "babel-plugin-import": "^1.13.3",
        "copyfiles": "^2.4.1",
        "rimraf": "^3.0.2"
    },
    "staticFiles": {
        "staticPath": "static",
        "watcherGlob": "**"
    }
}
